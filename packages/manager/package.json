{
  "name": "@openmcp/manager",
  "version": "0.0.0",
  "description": "Manage connections between MCP Servers and Clients through a single interface.",
  "type": "module",
  "license": "Apache-2.0",
  "sideEffects": false,
  "keywords": [
    "mcp",
    "model context protocol"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/getdatanaut/openmcp",
    "directory": "packages/manager"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "browser": "./dist/browser/index.js",
      "default": "./dist/node/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "test": "vitest run",
    "test.watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "1.2.10",
    "@ai-sdk/openai": "1.3.17",
    "@ai-sdk/provider": "1.1.3",
    "@modelcontextprotocol/sdk": "1.10.2",
    "@openmcp/utils": "workspace:*",
    "ai": "4.3.9",
    "dedent": "1.5.3",
    "lodash": "4.17.21",
    "nanoid": "5.1.5",
    "neverthrow": "8.2.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@libs/eslint-config": "workspace:*",
    "@libs/tsconfig": "workspace:*",
    "@types/lodash": "4.17.16",
    "@types/node": "22.14.1",
    "eslint": "9.25.1",
    "tsup": "8.4.0",
    "type-fest": "4.40.0",
    "typescript": "5.8.3",
    "vitest": "3.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
