{
  "name": "@openmcp/manager",
  "version": "0.0.0",
  "description": "Manage connections between MCP Servers and Clients through a single interface.",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/getdatanaut/openmcp",
    "directory": "packages/manager"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "test": "vitest run",
    "test.watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "1.2.5",
    "@ai-sdk/openai": "1.3.6",
    "@ai-sdk/provider": "1.1.0",
    "@modelcontextprotocol/sdk": "1.8.0",
    "@stoplight/json": "3.21.7",
    "ai": "4.2.11",
    "jsonpath-rfc9535": "1.2.0",
    "lodash": "4.17.21",
    "nanoid": "5.1.5",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@libs/eslint-config": "workspace:*",
    "@libs/tsconfig": "workspace:*",
    "@types/lodash": "4.17.16",
    "@types/node": "22.14.0",
    "eslint": "9.23.0",
    "tsup": "8.4.0",
    "type-fest": "^4.39.1",
    "typescript": "5.8.2",
    "vitest": "3.1.1"
  },
  "keywords": [
    "mcp",
    "model context protocol"
  ],
  "publishConfig": {
    "access": "public"
  }
}
