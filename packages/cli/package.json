{
  "name": "@openmcp/cli",
  "version": "0.0.1",
  "type": "module",
  "license": "Apache-2.0",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/getdatanaut/openmcp",
    "directory": "packages/cli"
  },
  "files": [
    "dist",
    "bin",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "package.json"
  ],
  "bin": {
    "openmcp": "./bin/index.js"
  },
  "imports": {
    "#register": {
      "types": "./src/register.ts",
      "development": "./src/register.ts",
      "default": "./dist/register.js"
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./rpc": {
      "types": "./src/rpc/index.ts",
      "development": "./src/rpc/index.ts",
      "default": "./dist/rpc/index.js"
    }
  },
  "scripts": {
    "exec": "NODE_ENV=development TS_NODE_TRANSPILE_ONLY=1 node --conditions=development --no-warnings --loader ts-node/esm ./bin/index.js",
    "build": "tsup",
    "lint": "eslint .",
    "test": "vitest run",
    "test.watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@better-fetch/fetch": "1.1.18",
    "@modelcontextprotocol/sdk": "1.10.2",
    "@openmcp/host-utils": "workspace:*",
    "@openmcp/openapi": "workspace:*",
    "@openmcp/remix": "workspace:*",
    "@openmcp/schemas": "workspace:*",
    "@orpc/client": "1.1.1",
    "@orpc/contract": "1.1.1",
    "better-auth": "1.2.7",
    "consola": "~3.4.2",
    "envalid": "~8.0.0",
    "open": "~10.1.1",
    "yargs": "~17.7.2",
    "yargs-parser": "~21.1.1",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@libs/eslint-config": "workspace:*",
    "@libs/tsconfig": "workspace:*",
    "@types/yargs": "^17.0.33",
    "@types/yargs-parser": "^21.0.3",
    "eslint": "9.25.1",
    "msw": "2.7.5",
    "ts-node": "^10.9.2",
    "tsup": "8.4.0",
    "typescript": "5.8.3",
    "vitest": "3.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
