{
  "name": "openmcp",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/getdatanaut/openmcp",
    "directory": "packages/cli"
  },
  "files": [
    "dist",
    "bin",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "package.json"
  ],
  "bin": "./bin/index.js",
  "imports": {
    "#libs/console": {
      "types": "./src/libs/console/index.ts",
      "development": "./src/libs/console/index.ts",
      "default": "./dist/libs/console/index.js"
    },
    "#libs/datanaut-auth": {
      "types": "./src/libs/datanaut/auth/index.ts",
      "development": "./src/libs/datanaut/auth/index.ts",
      "default": "./dist/libs/datanaut/auth/index.js"
    },
    "#libs/datanaut-auth-cli": {
      "types": "./src/libs/datanaut/auth-cli.ts",
      "development": "./src/libs/datanaut/auth-cli.ts",
      "default": "./dist/libs/datanaut/auth-cli.js"
    },
    "#libs/openapi": {
      "types": "./src/libs/openapi/index.ts",
      "development": "./src/libs/openapi/index.ts",
      "default": "./dist/libs/openapi/index.js"
    },
    "#register": {
      "types": "./src/register.ts",
      "development": "./src/register.ts",
      "default": "./dist/register.js"
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./src/api.ts",
      "development": "./src/api.ts",
      "default": "./dist/api.js"
    },
    "./rpc": {
      "types": "./src/rpc/index.ts",
      "development": "./src/rpc/index.ts",
      "default": "./dist/rpc/index.js"
    }
  },
  "scripts": {
    "exec": "NODE_ENV=development TS_NODE_TRANSPILE_ONLY=1 node --conditions=development --no-warnings --loader ts-node/esm ./bin/index.js",
    "build": "tsup",
    "lint": "eslint .",
    "test": "vitest run",
    "test.watch": "vitest watch",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@better-fetch/fetch": "1.1.18",
    "@clack/prompts": "0.10.1",
    "@modelcontextprotocol/sdk": "1.10.2",
    "@openmcp/host-utils": "workspace:*",
    "@openmcp/openapi": "workspace:*",
    "@openmcp/remix": "workspace:*",
    "@openmcp/schemas": "workspace:*",
    "@orpc/client": "1.1.1",
    "@orpc/contract": "1.1.1",
    "@sindresorhus/slugify": "^2.2.1",
    "@stoplight/http-spec": "7.1.0",
    "@stoplight/json": "3.21.7",
    "@stoplight/types": "14.1.1",
    "@stoplight/yaml": "4.3.0",
    "better-auth": "1.2.7",
    "consola": "~3.4.2",
    "envalid": "~8.0.0",
    "lodash-es": "^4.17.21",
    "open": "~10.1.1",
    "picocolors": "^1.1.1",
    "rotating-file-stream": "^3.2.6",
    "signal-exit": "^4.1.0",
    "yargs": "~17.7.2",
    "yargs-parser": "~21.1.1",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@datanaut/eslint-config": "0.1.1",
    "@datanaut/tsconfig": "0.1.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "22.15.3",
    "@types/yargs": "^17.0.33",
    "@types/yargs-parser": "^21.0.3",
    "eslint": "9.25.1",
    "msw": "2.7.5",
    "ts-node": "^10.9.2",
    "tsup": "8.4.0",
    "typescript": "5.8.3",
    "vitest": "3.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
