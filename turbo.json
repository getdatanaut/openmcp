{
  "$schema": "./node_modules/turbo/schema.json",
  "ui": "stream",
  "globalEnv": ["DEV", "DN_PG_URL"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "deploy": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": {},
    "test.watch": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "typegen": {},
    "migrate.up": {
      "cache": false
    },
    "dev.reset": {
      "cache": false
    },
    "dev.seed": {
      "cache": false
    }
  }
}
