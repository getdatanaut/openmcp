{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -c auto --names app,zero \"yarn dev.app\" \"yarn dev.zero\"",
    "dev.app": "vite --port=3001",
    "dev.zero": "zero-cache-dev -p shared/zero-schema.ts",
    "build": "CLOUDFLARE_ENV=production vite build",
    "build.analyze": "CLOUDFLARE_ENV=production vite build -- --analyze",
    "preview": "concurrently -c auto --names app,zero \"vite preview --port=3001\" \"yarn dev.zero\"",
    "deploy": "yarn build && wrangler deploy",
    "typecheck": "tsc -b",
    "lint": "eslint .",
    "typegen": "wrangler types",
    "generate.zero-schema": "drizzle-zero generate -f -c shared/drizzle-zero.config.ts -o shared/zero-schema.gen.ts -t tsconfig.shared.json"
  },
  "dependencies": {
    "@ai-sdk/react": "1.2.9",
    "@ai-sdk/ui-utils": "1.2.8",
    "@ariakit/react": "0.4.17",
    "@ariakit/react-core": "0.4.17",
    "@better-fetch/fetch": "1.1.18",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-regular-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@libs/api-contract": "workspace:*",
    "@libs/auth": "workspace:*",
    "@libs/db-pg": "workspace:*",
    "@libs/schemas": "workspace:*",
    "@libs/ui-primitives": "workspace:*",
    "@libs/utils-ids": "workspace:*",
    "@maxmorozoff/try-catch-tuple": "0.1.2",
    "@openmcp/cli": "workspace:*",
    "@openmcp/manager": "workspace:*",
    "@openmcp/openapi": "workspace:*",
    "@orpc/client": "1.1.1",
    "@orpc/openapi": "1.1.1",
    "@orpc/react-query": "1.1.1",
    "@orpc/server": "1.1.1",
    "@rocicorp/zero": "0.19.2025042700",
    "@tanstack/react-query": "5.74.7",
    "@tanstack/react-router": "1.117.1",
    "@tanstack/react-router-devtools": "1.117.1",
    "@zedux/react": "2.0.0-rc.7",
    "ai": "4.3.10",
    "better-auth": "1.2.7",
    "dayjs": "1.11.13",
    "drizzle-zero": "0.9.2",
    "jose": "5.10.0",
    "lodash": "4.17.21",
    "neverthrow": "8.2.0",
    "postgres": "3.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "10.1.0",
    "react-textarea-autosize": "8.5.9",
    "tailwind-merge": "3.2.0",
    "tailwind-variants": "1.0.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.0.12",
    "@libs/eslint-config": "workspace:*",
    "@libs/host-utils": "workspace:*",
    "@tailwindcss/vite": "4.1.4",
    "@tanstack/eslint-plugin-router": "1.115.0",
    "@tanstack/router-plugin": "1.117.2",
    "@types/lodash": "4.17.16",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.4.1",
    "concurrently": "9.1.2",
    "eslint": "9.25.1",
    "tailwindcss": "4.1.4",
    "typescript": "5.8.3",
    "vite": "6.3.3",
