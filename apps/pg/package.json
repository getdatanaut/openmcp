{
  "name": "pg",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint .",
    "typecheck": "tsc",
    "dev.seed": "dotenv -- tsx src/seed/index.ts",
    "dev.reset": "dotenv -- tsx src/reset.ts && yarn migrate.up",
    "dev.push": "dotenv -- drizzle-kit push",
    "--have not committed migrations yet, so migrate.up is just push atm--": "",
    "migrate.up": "dotenv -- drizzle-kit push",
    "migrate.up.eventually": "dotenv -- tsx src/migrate.ts",
    "migrate.generate": "dotenv -- drizzle-kit generate",
    "migrate.check": "dotenv -- drizzle-kit check",
    "drizzle-kit": "dotenv -- drizzle-kit"
  },
  "dependencies": {
    "@libs/db-pg": "workspace:*",
    "@libs/db-pg-migrations": "workspace:*",
    "drizzle-kit": "0.30.6",
    "kysely": "0.27.6"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "eslint": "9.24.0",
    "tsx": "4.19.3"
  }
}
